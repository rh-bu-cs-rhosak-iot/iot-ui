{
  "name": "meters-ui",
  "version": "1.0.0",
  "description": "UI application for meters",
  "main": "index.js",
  "scripts": {
    "build": "BUILD_PATH='../dist/client/build' craco build",
    "format": "prettier src/*.tsx src/**/*.tsx src/*.ts src/**/*.ts --trailing-comma=none --single-quote --write",
    "lint": "prettier src/*.tsx src/**/*.tsx src/*.ts src/**/*.ts --trailing-comma=none --single-quote --check"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@craco/craco": "^6.1.2",
    "google-map-react": "^2.1.10",
    "graphql": "^15.5.1",
    "moment": "^2.29.1",
    "query-string": "~6.13.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@types/google-map-react": "^2.1.2",
    "@types/googlemaps": "^3.43.3",
    "@types/node": "^12.20.15",
    "@types/react": "^17.0.12",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.1.7",
    "autoprefixer": "^9.8.6",
    "postcss": "^7.0.36",
    "prettier": "^2.3.2",
    "react-scripts": "4.0.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.4",
    "typescript": "^4.3.5"
  }
}
