{
  "name": "meters-proxy",
  "version": "1.0.0",
  "description": "Proxy server for meters application",
  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "nodemon --config nodemon.json --exec 'ts-node' dist/server.js",
    "format": "prettier src/**/*.ts --trailing-comma=none --single-quote --write",
    "lint": "prettier src/**/*.ts --trailing-comma=none --single-quote --check"
  },
  "license": "MIT",
  "dependencies": {
    "env-var": "^7.0.0",
    "fastify": "^3.18.1",
    "fastify-http-proxy": "^6.0.0",
    "fastify-plugin": "^3.0.0",
    "fastify-static": "^4.2.2",
    "pino": "^6.11.2"
  },
  "devDependencies": {
    "@types/node": "^12.20.7",
    "@types/pino": "^6.3.8",
    "prettier": "^2.3.2",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
